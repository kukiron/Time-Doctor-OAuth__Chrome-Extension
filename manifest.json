{
  "name": "Time Doctor OAuth 2.0",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "33",

  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "key": "",
  
  "background": {
    "scripts": ["background.js"]  
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "images/icon70.png",
    "default_title": "Time Doctor Authentication"
  },
  "permissions": [
    "identity",
    "*://*.google.com/*",    
    "*://*.timedoctor.com/*",
    "contextMenus"
  ]
}